int main() {
    int a, b;
    a = 3;
    b = 5;
    const int c; 
    c = a + (b - 7);
    printf(c);
    int d = c * b;
    if(c == a){
        int d = d - c;
        printf(d);
    } else {
        b = d / 4;
    }
    printf(b);
    printf(d);
    while(a < b){
        int a = a + 1;
    }
    printf(a);
}